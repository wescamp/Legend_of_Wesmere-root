msgid "ice shot"
msgstr ""

#: Legend_of_Wesmere/units/Drake_Mage.cfg:68
msgid "cold"
msgstr "kulde"

#: Legend_of_Wesmere/units/Drake_Mage.cfg:72
msgid "magical"
